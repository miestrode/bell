fn is_prime(number: int) {
    let current = 2;
    let is_prime = true;

    while current < number {
        if number % current == 0 {
            is_prime = false; // No early returns yet
        }
        
        current = current + 1;
    }
    
    is_prime
}

fn main() {
    let current = 2;
    
    while current < 100 {
        if is_prime(current) {
            println(current);
        }
        
        current = current + 1;
    }
}
